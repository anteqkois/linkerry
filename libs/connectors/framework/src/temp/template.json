    "name": "ChatGPT SNS Customer Service Agent",
    "description": "This flow creates an SMS customer service agent powered by ChatGPT. It incorporates memory of the chat history and it's contextualized to the business. ",
    "projectId": "LqcJP3WXwaGLUtLZos6Qo",
    "template": {
      "id": "v4gnzyCPkbjcbIttlKub8",
      "state": "DRAFT",
      "valid": false,
      "flowId": "Y1Tb3uIqGPvmPiX6R7giO",
      "created": "2023-07-25T12:09:24.623Z",
      "trigger": {
        "name": "trigger",
        "type": "WEBHOOK",
        "valid": true,
        "settings": {
          "inputUiInfo": {}
        },
        "nextAction": {
          "name": "step_3",
          "type": "PIECE",
          "valid": true,
          "settings": {
            "input": {
              "key": "history_{{trigger.body.From.replace(\"+\",\"\")}}",
              "store_scope": "FLOW"
            },
            "pieceName": "@activepieces/piece-store",
            "actionName": "get",
            "inputUiInfo": {},
            "pieceVersion": "~0.2.6"
          },
          "nextAction": {
            "name": "step_1",
            "type": "PIECE",
            "valid": false,
            "settings": {
              "input": {
                "prompt": "Act as a competent and friendly customer support agent who works at our business. You are the Agent and you are chatting with User. Complete the responses of the chat using the business information in the Business Details section. Carry on the conversation like you are chatting, don't make a conversation opener every time you chat unless necessary. If you can't the information in the Business Details, be conservative with your answer, don't give false information to User.\n\nBusiness Details\nStoreX is an IT repair shop, we specialize in repairing Apple devices. Only new devices are accepted. We open all days from 8am PT to 10pm PT. We're located in San Francisco and we're opening new branches soon, no locations are specified.\n\nChat History\n{{step_3}}\nUser: {{trigger.body.Body}}\nAgent: "
              },
              "pieceName": "@activepieces/piece-openai",
              "actionName": "ask_chatgpt",
              "inputUiInfo": {},
              "pieceVersion": "~0.2.5"
            },
            "nextAction": {
              "name": "step_2",
              "type": "PIECE",
              "valid": false,
              "settings": {
                "input": {
                  "to": "{{trigger.body.From}}",
                  "body": "{{step_1}}",
                  "from": "+17873392236"
                },
                "pieceName": "@activepieces/piece-twilio",
                "actionName": "send_sms",
                "inputUiInfo": {},
                "pieceVersion": "~0.2.1"
              },
              "nextAction": {
                "name": "step_4",
                "type": "PIECE",
                "valid": true,
                "settings": {
                  "input": {
                    "key": "history_{{trigger.body.From.replace(\"+\", \"\")}}",
                    "value": "{{step_3}}\nUser: {{trigger.body.Body}}\nAgent: {{step_1}}",
                    "store_scope": "FLOW"
                  },
                  "pieceName": "@activepieces/piece-store",
                  "actionName": "put",
                  "inputUiInfo": {},
                  "pieceVersion": "~0.2.6"
                },
                "displayName": "Put"
              },
              "displayName": "Send SMS"
            },
            "displayName": "Ask ChatGPT"
          },
          "displayName": "Get"
        },
        "displayName": "Webhook Trigger"
      },
      "updated": "2023-08-05T14:29:34.095Z",
